{
  "name": "tiny-chat",
  "version": "1.1.1-alpha",
  "description": "a simple chat gpt client",
  "main": "./out/main/index.js",
  "author": "jaken",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@milkdown/core": "^7.2.4",
    "@milkdown/ctx": "^7.2.4",
    "@milkdown/plugin-listener": "^7.2.4",
    "@milkdown/plugin-prism": "^7.2.4",
    "@milkdown/plugin-slash": "^7.2.4",
    "@milkdown/plugin-trailing": "^7.2.4",
    "@milkdown/preset-commonmark": "^7.2.4",
    "@milkdown/prose": "^7.2.4",
    "@milkdown/theme-nord": "^7.2.4",
    "@milkdown/transformer": "^7.2.4",
    "@milkdown/utils": "^7.2.4",
    "@milkdown/vue": "^7.2.4",
    "highlight.js": "^11.7.0",
    "loadsh": "^0.0.4",
    "pinia": "^2.0.33",
    "prism-themes": "^1.9.0",
    "uuid": "^9.0.0",
    "vue-github-button": "^3.1.0",
    "vue-markdown-render": "^2.0.1",
    "vue-router": "4"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "16.18.21",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.4.14",
    "axios": "^1.3.4",
    "electron": "^22.3.4",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.21",
    "eslint": "^8.36.0",
    "eslint-plugin-vue": "^9.10.0",
    "font-list": "^1.5.0",
    "naive-ui": "^2.34.3",
    "postcss": "^8.4.27",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.9.5",
    "vite": "^4.2.1",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0"
  }
}
